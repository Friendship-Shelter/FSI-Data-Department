CREATE TABLE data_department.bnl
(
    ind integer NOT NULL,
    uid text NOT NULL,
    pid integer NOT NULL,
    eid integer NOT NULL,
    cname text
    start_date date NOT NULL,
    exit_date date,
    hoh text
    age integer,
    ethnicity text
    gender text
    race text
    veteran text
    program_city text 
    chronic text
    episode_start_date date,
    city_last_address text
    city_immediately_prior text
    destination text
    any_disability text
    chronic_disability text
    developmental_disability text
    hiv_disability text
    mh_disability text
    physical_disability text
    sud_disability text
    pname text
    ptype text
    current_homelessness_city text
    CONSTRAINT "BNL_pkey" PRIMARY KEY ind)
)

TABLESPACE pg_default;

ALTER TABLE data_department.bnl
    OWNER to postgres;
